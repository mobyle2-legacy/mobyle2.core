<metal:define metal:define-macro="main">
<div id="globaltabs">
  <ul>
    <tal:r tal:repeat="i python:v.get_globaltabs()">
    <li tal:attributes="class python:i['class']">
    <a tal:attributes="href python:i['url']" tal:content="python:i['name']"></a>
    </li> 
    </tal:r>
  </ul>
</div>
</metal:define>
