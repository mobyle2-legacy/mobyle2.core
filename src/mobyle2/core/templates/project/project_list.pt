<metal metal:use-macro="main.macros['main']">
  <metal metal:fill-slot="content_body"> 
    <h1>List of projects</h1>
    <ul tal:define="items c.items">
      <tal:r tal:repeat="name python:items">
        <li tal:define="project items[name].project">
          <a tal:attributes="href python:u(items[name])" >${project.id} (${project.name})</a>
        </li>
      </tal:r>
    </ul>
  </metal>
</metal>
