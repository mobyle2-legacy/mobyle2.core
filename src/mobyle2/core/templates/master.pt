<metal:define metal:define-macro="main">
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<html 
  xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
  xmlns:i18n="http://xml.zope.org/namespaces/i18n"
  i18n:domain="mobyle2">
  <head>
    <metal:define metal:define-slot="head">
    <metal:define metal:define-slot="title">
    <title>Mobyle 2</title>
    </metal:define>
    <meta http-equiv="Content-Type" content="text/html;charset=UTF-8"/>
    <link rel="shortcut icon"  tal:attributes="href request.static_url('mobyle2.core:static/pylons.ico')"/>
    <metal:define metal:define-slot="js_slot">
    <script type="text/javascript" tal:attributes="src python:request.static_url('mobyle2.core:static/js/jquery-1.7.min.js')"></script>
    <script type="text/javascript" tal:attributes="src python:request.static_url('deform:static/scripts/jquery-1.4.2.min.js')"></script>
    <script type="text/javascript" tal:attributes="src python:request.static_url('deform:static/scripts/deform.js')"></script>
    <script type="text/javascript" tal:attributes="src python:request.static_url('deform:static/scripts/jquery.form.js')"></script>
    </metal:define>
    <metal:define metal:define-slot="css_slot">
    <link rel="stylesheet" type="text/css" tal:attributes="href python:request.static_url('mobyle2.core:static/mobyle2.css')"/>
    <link rel="stylesheet" type="text/css" tal:attributes="href python:request.static_url('deform:static/css/form.css')"/>
    </metal:define>
    </metal:define>
  </head>
  <metal:define metal:define-macro="body">
  <body>
    <div id="wrap">
      <metal:define metal:define-slot="header">
      <div id="header">
        <metal:define metal:define-slot="header_body">
        <div class="banner-wrap">
          <metal:define metal:use-macro="banner.macros['main']"/>
        </div>
        <div class="global_tabs-wrap">
          <metal:define metal:use-macro="globaltabs.macros['main']">
        </div>
        <div class="breadcrumbs-wrap">
          <metal:define metal:use-macro="breadcrumbs.macros['main']"/>
        </div>
        </metal:define>
      </div>
      </metal:define>
      <div id="content">
        <metal:define metal:define-slot="content">
        <metal:define metal:define-slot="right_slot">
        <div class="right_slot">
        </div>
        </metal:define>
        <metal:define metal:define-slot="middle_slot">
        <div class="middle_slot">
          <div id="above-content">
            <metal:define metal:define-slot="above_content">
            </metal:define>
          </div>
          <div id="content-body">
            <metal:define metal:define-slot="content_body">
            </metal:define>
          </div>
          <div id="bottom">
            <metal:define metal:define-slot="below_content">
            </metal:define>
          </div>
        </div>
        </metal:define>
        <metal:define metal:define-slot="left_slot">
        <div class="left_slot">
        </div>
        </metal:define>
        </metal:define>
      </div>
      <div id="bottom">
        <metal:define metal:define-slot="footer">
        <div id="footer">
          <metal:define metal:define-slot="footer_body">
          </metal:define>
          <metal:define metal:define-slot="colophon">
          </metal:define>
        </div>
        </metal:define>
      </div>
    </div>
    <p i18n:translate="">Home</p>
  </body>
  </metal:define>
</html>
</metal:define>
